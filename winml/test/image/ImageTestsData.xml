<?xml version="1.0" ?>
<Data>
    <Table ID="Devices">
        <ParameterTypes>
            <ParameterType Name="Device">String</ParameterType>
        </ParameterTypes>
        <Row Name="GPU">
            <Parameter Name="Device">GPU</Parameter>
        </Row>
        <Row Name="CPU">
            <Parameter Name="Device">CPU</Parameter>
        </Row>
    </Table>

    <Table ID="Models">
        <ParameterTypes>
            <ParameterType Name="ModelFileName">String</ParameterType>
            <ParameterType Name="ModelInputOutputType">String</ParameterType>
            <ParameterType Name="ModelPixelFormat">String</ParameterType>
        </ParameterTypes>
        <!-- We need a model takes Gray8 image as input-->
        <Row Name="FNS-candy_Bgr8">
            <Parameter Name="ModelFileName">fns-candy_Bgr8.onnx</Parameter>
            <Parameter Name="ModelInputOutputType">image</Parameter>
            <Parameter Name="ModelPixelFormat">Bgr8</Parameter>
        </Row>
        <Row Name="FNS-candy_Rgb8">
            <Parameter Name="ModelFileName">fns-candy_Rgb8.onnx</Parameter>
            <Parameter Name="ModelInputOutputType">image</Parameter>
            <Parameter Name="ModelPixelFormat">Rgb8</Parameter>
        </Row>
        <Row Name="FNS-candy_Tensor">
            <Parameter Name="ModelFileName">fns-candy_tensor.onnx</Parameter>
            <Parameter Name="ModelInputOutputType">tensor</Parameter>
            <Parameter Name="ModelPixelFormat">Bgr8</Parameter>
        </Row>
        <Row Name="FNS-candy_Bgr8_freeDimInput">
            <Parameter Name="ModelFileName">fns-candy_Bgr8_freeDimInput.onnx</Parameter>
            <Parameter Name="ModelInputOutputType">image</Parameter>
            <Parameter Name="ModelPixelFormat">Bgr8</Parameter>
        </Row>
    </Table>

    <Table ID="InputImageFiles">
         <ParameterTypes>
            <ParameterType Name="ImageFileName">String</ParameterType>
        </ParameterTypes>
        <Row Name="ball_1080">
            <Parameter Name="ImageFileName">1080.jpg</Parameter>
        </Row>
        <Row Name="kitten_224">
            <Parameter Name="ImageFileName">kitten_224.png</Parameter>
        </Row>
        <Row Name="fish_720">
            <Parameter Name="ImageFileName">fish_720.png</Parameter>
        </Row>
        <Row Name="fish_720_Gray">
            <Parameter Name="ImageFileName">fish_720_Gray.png</Parameter>
        </Row>
    </Table>

    <Table ID="OutputBindingStrategies">
        <ParameterTypes>
            <ParameterType Name="OutputBindingStrategy">String</ParameterType>
        </ParameterTypes>
        <Row Name="Bound">
            <Parameter Name="OutputBindingStrategy">bound</Parameter>
        </Row>
        <Row Name="Unbound">
            <Parameter Name="OutputBindingStrategy">unbound</Parameter>
        </Row>
    </Table>


    <Table ID="InputImageSources">
        <ParameterTypes>
            <ParameterType Name="InputImageSource">String</ParameterType>
        </ParameterTypes>
        <Row Name="FromImageFeatureValue">
            <Parameter Name="InputImageSource">FromImageFeatureValue</Parameter>
        </Row>
        <Row Name="FromImageVideoFrame">
            <Parameter Name="InputImageSource">FromVideoFrame</Parameter>
        </Row>
        <Row Name="ImageFromCPUResource">
            <Parameter Name="InputImageSource">FromCPUResource</Parameter>
        </Row>
        <Row Name="ImageFromGPUResource">
            <Parameter Name="InputImageSource">FromGPUResource</Parameter>
        </Row>
    </Table>

    <Table ID="InputPixelFormats">
        <ParameterTypes>
            <ParameterType Name="InputPixelFormat">String</ParameterType>
        </ParameterTypes>
        <Row Name="Bgra8">
            <Parameter Name="InputPixelFormat">Bgra8</Parameter>
        </Row>
        <Row Name="Rgba8">
            <Parameter Name="InputPixelFormat">Rgba8</Parameter>
        </Row>
        <Row Name="Gray8">
            <Parameter Name="InputPixelFormat">Gray8</Parameter>
        </Row>
    </Table>

    <Table ID="EvaluationStrategies">
        <ParameterTypes>
            <ParameterType Name="EvaluationStrategy">String</ParameterType>
        </ParameterTypes>
        <Row Name="Async">
            <Parameter Name="EvaluationStrategy">Async</Parameter>
        </Row>
        <Row Name="Sync">
            <Parameter Name="EvaluationStrategy">Sync</Parameter>
        </Row>
    </Table>
</Data>
